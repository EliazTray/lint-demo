{
  "name": "lint-demo",
  "version": "0.0.0",
  "description": "my gnarly project",
  "main": "dist/lint-demo.cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "standard",
    "prepublishOnly": "npm run build",
    "build": "bili"
  },
  "repository": {
    "url": "eliatray/lint-demo",
    "type": "git"
  },
  "author": "eliatray<944126009@qq.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "prettier": "^1.15.2",
    "standard": "^12.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.0",
    "husky": "^1.0.0-rc.13",
    "bili": "^3.3.0"
  },
  "standard": {
    "env": "jest"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
