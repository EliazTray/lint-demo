{
  "name": "lint-demo",
  "version": "0.0.0",
  "description": "my gnarly project",
  "main": "dist/lint-demo.cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "standard",
    "prepublishOnly": "npm run build",
    "build": "bili",
    "dev": "nodemon src/index.js"
  },
  "repository": {
    "url": "eliatray/lint-demo",
    "type": "git"
  },
  "author": "eliatray<944126009@qq.com>",
  "license": "MIT",
  "dependencies": {
    "cosmiconfig": "^5.1.0",
    "dumper.js": "^1.3.1",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "postcss": "^7.0.14",
    "postcss-selector-parser": "^6.0.2"
  },
  "devDependencies": {
    "bili": "^3.3.0",
    "chalk": "^2.4.2",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.15.2",
    "raw-loader": "^2.0.0",
    "signale": "^1.4.0",
    "standard": "^12.0.0",
    "style-search": "^0.1.0"
  },
  "standard": {
    "env": "jest"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
